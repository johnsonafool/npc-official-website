{
  "type": "module",
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">=20.17.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && pnpm exec simple-git-hooks"
  },
  "dependencies": {
    "nuxt": "3.13.1",
    "vue": "3.5.2",
    "vue-router": "4.4.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.2.2",
    "eslint": "9.9.1",
    "eslint-plugin-format": "0.1.2",
    "lint-staged": "15.2.10",
    "simple-git-hooks": "2.11.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
